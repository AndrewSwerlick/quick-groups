{
    "manifest_version": 3,
    "name": "Quick Groups",
    "version": "0.1",
    "permissions":[
        "tabs",
        "tabGroups",
        "bookmarks"
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "groups.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
